class arryas{
public static void main(String[] args) {
        int[]num={1,2,3,4,5,6};
        int temp;
        for(int i=0;i<num.length;i++){
            for(int j=0;j<(num.length-1)/2;j++){
                if(num[j] < num[j+1]) {
                    temp = num[j];
                    num[j] = num[j + 1];
                    num[j + 1] = temp;
                }
            }
        }
        for(int i=0;i<num.length;i++){
            System.out.print(num[i]);
        }
    }
    }
